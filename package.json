{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "dev": "npm run compile && concurrently --kill-others \"npm run watch\" \"npm start\"",
    "watch": "watchify app/App.js -t babelify -o bundle.js --debug --verbose",
    "compile": "browserify app/App.js -t babelify -o bundle.js --debug --verbose",
    "linter": "node ./node_modules/eslint/bin/eslint.js app --ext .js  --cache --env jquery"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "concurrently": "^3.4.0",
    "electron": "^1.4.1",
    "electron-reload": "^1.1.0",
    "eslint": "^3.17.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-react": "^6.10.0",
    "watchify": "^3.9.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "electron-context-menu": "^0.8.0",
    "phantom": "^4.0.0",
    "phantomjs-prebuilt": "^2.1.14",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "uuid": "^3.0.1",
    "webpage": "^0.3.0"
  }
}
